    "version": "0.2.0",
    "configurations": [
        {
            "name": "movies_tmdb (dev mode)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": ["--debug --dart-define=ENVIRONMENT=DEV .env"]
        },
        {
            "name": "movies_tmdb (profile mode)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": ["--profile --dart-define=ENVIRONMENT=PROD .env"]
        },
        {
            "name": "movies_tmdb (profile mode) Prod",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": ["--verbose  --dart-define=ENVIRONMENT=PROD .env"]
            
        },
        {
            "name": "movies_tmdb (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["--dart-define=ENVIRONMENT=PROD .env"]
        }
    ]
}